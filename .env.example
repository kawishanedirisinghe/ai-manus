# AI Manus Environment Configuration
# Copy this file to .env and fill in your actual values

# =================
# API Configuration  
# =================

# OpenAI API Keys (add more as needed)
OPENAI_API_KEY_1=sk-your-openai-key-1
OPENAI_API_KEY_2=sk-your-openai-key-2
OPENAI_API_KEY_3=sk-your-openai-key-3

# Anthropic API Keys
ANTHROPIC_API_KEY_1=sk-ant-your-anthropic-key-1
ANTHROPIC_API_KEY_2=sk-ant-your-anthropic-key-2

# Google API Keys (Gemini)
GOOGLE_API_KEY_1=AIzaSyXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
GOOGLE_API_KEY_2=AIzaSyXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
GOOGLE_API_KEY_3=AIzaSyXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
GOOGLE_API_KEY_4=AIzaSyXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
GOOGLE_API_KEY_5=AIzaSyXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX

# DeepSeek API Keys
DEEPSEEK_API_KEY_1=sk-your-deepseek-key-1
DEEPSEEK_API_KEY_2=sk-your-deepseek-key-2

# =================
# Application Settings
# =================

# Environment
ENVIRONMENT=development
APP_NAME=ai-manus
APP_VERSION=1.0.0
LOG_LEVEL=INFO
LOG_FORMAT=json

# Rate Limiting
RATE_LIMIT_ENABLED=true
MAX_REQUESTS_PER_MINUTE=1000
MAX_TOKENS_PER_MINUTE=100000

# Retry Configuration
RETRY_ATTEMPTS=3
RETRY_DELAY=1.0
BACKOFF_FACTOR=2.0

# =================
# Database & Storage
# =================

# MongoDB
MONGODB_URI=mongodb://localhost:27017
MONGODB_DATABASE=ai_manus
MONGODB_USERNAME=
MONGODB_PASSWORD=

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# =================
# External Services
# =================

# Web Search (Google Custom Search)
GOOGLE_SEARCH_API_KEY=your-search-api-key
GOOGLE_SEARCH_ENGINE_ID=your-search-engine-id

# Search Provider (baidu, google)
SEARCH_PROVIDER=baidu

# Browser Configuration
BROWSER_API_KEY=your-browser-api-key

# =================
# Security
# =================

SECRET_KEY=your-super-secret-key-change-this-in-production
JWT_SECRET=your-jwt-secret-key-change-this-in-production

# =================
# Docker Configuration
# =================

# Sandbox Configuration
SANDBOX_IMAGE=simpleyyt/manus-sandbox
SANDBOX_NAME_PREFIX=sandbox
SANDBOX_TTL_MINUTES=30
SANDBOX_NETWORK=manus-network
SANDBOX_CHROME_ARGS=
SANDBOX_HTTPS_PROXY=
SANDBOX_HTTP_PROXY=
SANDBOX_NO_PROXY=

# Docker
DOCKER_COMPOSE_FILE=docker-compose.yml
DOCKER_BUILDX_NO_DEFAULT_ATTESTATIONS=1

# =================
# MCP Configuration
# =================

MCP_CONFIG_PATH=/etc/mcp.json